# editorconfig.org
root = true

[*]
indent_style = space
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_size = 2

[*.vue]
ij_javascript_use_double_quotes = false

[*.php]
indent_size = 4

[*.js]
indent_size = 2

[*.md]
trim_trailing_whitespace = false
